   "id": "9600ca0f",
   "metadata": {},
   "outputs": [],
   "source": [
    "hulu_movies = df_hulu.loc[df_hulu['type'] == 'Movie']\n",
    "hulu_tv_shows = df_hulu.loc[df_hulu['type'] == 'TV Show']"
   ]
  },
